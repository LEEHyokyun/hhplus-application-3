<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.hhplus.be.server.infrastructure.mybatis.point">
  <select id="search" resultType="PointDTO">
    SELECT P.POINT
    FROM POINT P
    WHERE P.USER_ID = #{userId}
  </select>
</mapper>